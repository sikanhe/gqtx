{
  "name": "gqtx",
  "version": "0.8.0",
  "type": "module",
  "main": "cjs/index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./cjs/index.js"
    },
    "./relay": {
      "import": "./relay.js",
      "require": "./cjs/relay.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sikanhe/gqtx"
  },
  "license": "MIT",
  "keywords": [
    "graphql",
    "typescript",
    "graphql-server"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/express": "^4.17.1",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.1.0",
    "rollup": "^2.39.0",
    "riteway": "^6.2.1",
    "ts-node": "^8.4.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "graphql": "^15.1.0"
  },
  "scripts": {
    "build:cjs": "MODE=cjs rollup -c",
    "build:esm": "rollup -c",
    "build": "yarn build:cjs && yarn build:esm",
    "test": "riteway --require ts-node/register/transpile-only src/**/*.test.ts"
  }
}
